<odoo>
    <data noupdate="1">

        <!--智能人事-->
        <record id="dingding_parameter_hrm_list" model="dingding.parameter">
            <field name="name">获取员工花名册字段信息</field>
            <field name="key">hrm_list</field>
            <field name="value">https://oapi.dingtalk.com/topapi/smartwork/hrm/employee/list?access_token=</field>
        </record>

        <record id="dingding_parameter_search_hrm_querypreentry" model="dingding.parameter">
            <field name="name">查询企业待入职员工列表</field>
            <field name="key">hrm_querypreentry</field>
            <field name="value">https://oapi.dingtalk.com/topapi/smartwork/hrm/employee/querypreentry?access_token=</field>
        </record>

        <record id="dingding_parameter_hrm_queryonjob" model="dingding.parameter">
            <field name="name">查询企业在职员工列表</field>
            <field name="key">hrm_queryonjob</field>
            <field name="value">https://oapi.dingtalk.com/topapi/smartwork/hrm/employee/queryonjob?access_token=</field>
        </record>

